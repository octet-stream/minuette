  "devDependencies": {
    "@octetstream/eslint-config": "3.0.0",
    "ava": "2.0.0",
    "codecov": "3.5.0",
    "eslint": "5.16.0",
    "eslint-plugin-ava": "7.0.0",
    "eslint-plugin-markdown": "1.0.0",
    "husky": "2.3.0",
    "lint-staged": "8.1.7",
    "nyc": "14.1.1"
  }
}
