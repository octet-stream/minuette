{
  "name": "minuette",
  "version": "0.1.2",
  "description": "A job scheduler with elegant syntax.",
  "keywords": [
    "job-scheduler",
    "scheduler",
    "runner",
    "job",
    "jobs",
    "timer",
    "cron",
    "chainable"
  ],
  "main": "lib/Minuette",
  "repository": "octet-stream/minuette",
  "author": "Nick K.",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "ava",
    "staged": "lint-staged",
